# ----------------
# LANGUAGE SYSTEMS
# ----------------

languagesystem DFLT dflt;   # Default
languagesystem latn dflt;   # Latin default
languagesystem cyrl dflt;   # Cyrillic default
languagesystem latn MOL;
languagesystem latn ROM;
languagesystem cyrl BGR;



# --------------
# GLOBAL CLASSES
# --------------

